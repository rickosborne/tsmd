		"@biomejs/biome": ">=2",
		"eslint": ">=9",
		"prettier": ">=3"
	},
	"private": false,
	"readme": "README.md",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rickosborne/tsmd.git"
	},
	"scripts": {
		"build": "tsx scripts/build.ts",
		"build:types": "tsc -p tsconfig.dts.json",
		"lint": "eslint",
		"test": "mocha",
		"type-check": "tsc --noEmit",
		"upgrade:check": "npm-check-updates"
	},
	"type": "module",
	"types": "./dist/index.d.ts",
	"version": "2025.8.29"
